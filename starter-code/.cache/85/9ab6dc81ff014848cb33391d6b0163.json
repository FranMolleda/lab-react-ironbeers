 React from \"react\";\nimport { BrowserRouter as Link } from \"react-router-dom\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport Navbar from \"./components/Navbar\";\nimport useFetch from \"./components/hooks/useFetch\";\nimport Cards from \"./components/UI/AllBeers\";\n\nconst Beers = function() {\n  const { data, loading } = useFetch(\n    \"https://ih-beers-api2.herokuapp.com/beers\"\n  );\n  if (loading) {\n    return <Cards>Cargando las de cervezas...</Cards>;\n  }\n\n  return (\n    <Cards>\n      <Navbar />\n      {data.map(beer => (\n        <React.Fragment key={beer._id}>\n          <div className=\"card\" style={{ width: \"24rem\", height: \"11rem\" }}>\n            <Link\n              to={`/beer-detail/${beer._id}`}\n              key={beer.i}\n              style={{ textDecoration: \"none\" }}\n            >\n              <div className=\"row no-gutters\">\n                <div className=\"col-md-4\">\n                  <img\n                    style={{ width: \"40px\", height: \"7rem\" }}\n                    src={beer.image_url}\n                    className=\"card-img mt-4 ml-4\"\n                    alt=\"Beer Picture\"\n                  />\n                </div>\n                <div className=\"col-md-8\" key={beer.i}>\n                  <div className=\"card-body\">\n                    <h5 className=\"card-title\">{beer.name}</h5>\n                    <p\n                      className=\"card-text\"\n                      style={{\n                        maxHeight: \"12rem\",\n                        overflow: \"hidden\",\n                        texOverflow: \"ellipsis\"\n                      }}\n                    >\n                      {beer.tagline}\n                    </p>\n                    <p className=\"card-text\">\n                      <small className=\"text-muted\">\n                        {beer.contributed_by}\n                      </small>\n                    </p>\n                  </div>\n                </div>\n              </div>\n            </Link>\n          </div>\n        </React.Fragment>\n      ))}\n    </Cards>\n  );\n};\n\nexport default Beers;\n"},"lineCount":null}},"error":null,"hash":"dd2cd2dc82900242428c4ca29802f966","cacheData":{"env":{}}}